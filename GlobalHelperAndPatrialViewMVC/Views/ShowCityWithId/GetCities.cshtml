@model IEnumerable<GlobalHelperAndPatrialViewMVC.Models.City>

@{
    ViewBag.Title = "GetCities";
}

<h2>GetCities</h2>

<table>
  

@foreach (var item in Model.ToList()) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
   
    </tr>
}

</table>
